{
  "extends": "@prismedis/tsconfig/base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
    },
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json",
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.json"
  ],
  "exclude": [
    "node_modules"
  ],
}